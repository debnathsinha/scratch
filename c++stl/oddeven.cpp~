#include <iostream>
#include <vector>
#include <string>

using namespace std;

string findOddEven( int i ) {
  if( i %2 == 0 ) {
    return "even";
  } else {
    return "odd";
  }
}

int main() {

  vector<int> in;
  vector<string> str;

  in.push_back(1);
  in.push_back(2);
  in.push_back(3);

  transform(in.begin(), in.end(), out.begin(), findOddEven);

}
