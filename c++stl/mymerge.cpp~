#include <iostream>
#include <vector>


void merge_array() {
  int a[1000];
  int b[2000];

  int c[3000];

  for(int i = 0; i < 1000; i++ )
    a[i] = i;

  for( int i = 0; i < 2000; i++ )
    b[i] = i;

  merge(a,a+1000,b,b+2000,c);


}


int main( int argc, char ** argv ) {

  if( argc != 1 ) 
    throw("Usage: mymerge");

  merge_array();

}
